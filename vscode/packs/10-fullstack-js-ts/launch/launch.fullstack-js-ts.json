{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug JS/TS baseline tests",
      "runtimeExecutable": "node",
      "runtimeArgs": [
        "--test",
        "${workspaceFolder}/examples/js-ts-baseline/test"
      ],
      "cwd": "${workspaceFolder}/examples/js-ts-baseline",
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Node: Current file",
      "program": "${file}",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}
